tegory}\n      </Typography>\n    </React.Fragment>\n  );\n\n  if (isLoading) {\n    DOM = (\n      <CircularProgress\n        data-test=\"component-loading\"\n        className={classes.loading}\n      />\n    );\n  } else if (data && matches) {\n    listPodcasts = data.feed.results.map((podcast) => {\n      return (\n        <Link\n          to={'/podcast/' + podcast.id}\n          key={podcast.id}\n          className={classes.link}\n        >\n          <PodcastCard\n            image={podcast.artworkUrl100}\n            artist={podcast.name}\n            artistName={podcast.artistName}\n            data-test=\"component-card\"\n          />\n        </Link>\n      );\n    });\n    categoryPodcasts = categoryData.results.map((podcast) => {\n      return (\n        <Link\n          to={'/podcast/' + podcast.collectionId}\n          key={podcast.id}\n          className={classes.link}\n        >\n          <PodcastCard\n            image={podcast.artworkUrl600}\n            artist={podcast.collectionName}\n            artistName={podcast.artistName}\n            data-test=\"component-card\"\n          />\n        </Link>\n      );\n    });\n    DOM = (\n      <React.Fragment>\n        {firstSection}\n        <PodcastsLayout isLoading={isLoading} podcasts={listPodcasts} />\n        {secondSection}\n        <PodcastsLayout isLoading={isLoading} podcasts={categoryPodcasts} />\n      </React.Fragment>\n    );\n  } else if (data) {\n    listPodcasts = data.feed;\n    DOM = (\n      <React.Fragment>\n        {firstSection}\n        <PodcastCarousel podcasts={listPodcasts} />\n        {secondSection}\n        <PodcastCarousel podcasts={categoryData} />\n      </React.Fragment>\n    );\n  }\n\n  return <div className={classes.rootHome}>{DOM}</div>;\n};\n\nexport default Home;\n","/home/davidf/Desktop/React/podcast-app/src/containers/Home/homeStyles.js",[],"/home/davidf/Desktop/React/podcast-app/src/configureStore.js",[],["205","206"],"/home/davidf/Desktop/React/podcast-ap